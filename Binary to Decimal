#num = input()
n = len(num)-1
decimal = 0
for i in num:
  decimal+=int(i)*(2**n)
  n-=1
print(decimal)


#manual method
bin = "1011"
decimal = 0
n = len(bin)
for i in range(n):
  decimal += int(bin[n-1-i])*(2**i)
print(decimal)



#direct methode
binary = "1001"
deci = 0
deci = int(binary,2)
print(deci)
